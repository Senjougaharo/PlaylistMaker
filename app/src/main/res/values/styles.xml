<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MessageTextStyle">
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">26sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
    </style>

    <style name="SmallMessageTextStyle">
        <item name="android:textColor">@color/black_as_fu</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
    </style>

    <style name="MessageHeadLine">
        <item name="android:textColor">@color/black</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
    </style>

    <style name="MainActivityBackground">
        <item name="android:background">@color/blue</item>
    </style>

    <style name="icons">
        <item name="android:fillColor">@color/grey</item>
    </style>

</resources>